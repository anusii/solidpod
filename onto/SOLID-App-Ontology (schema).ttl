###  http://sii.cecs.anu.edu.au/onto/SOLID-App#App
sa-onto:App rdf:type owl:Class ;
            rdfs:subClassOf foaf:Agent ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:hasFile ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass sa-onto:File
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:hasSharedKey ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass sa-onto:SharedKey
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:uses ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass sa-onto:POD
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dcterms:title ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:ivB64 ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:privateKey ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:publicKey ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sa-onto:securityKeyCheck ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment """The IRI of an *App* instance MUST be in the following format:
- [https://]  # with an HTTPS scheme.
- {server URL}/{POD name given by the user (*Person*)}/{APP name}/

Example:
https://pods.solidcommunity.au/user123-POD/appName/, where
{server URL} = \"pods.solidcommunity.au\"
{POD name given by the user (*Person*)} = \"user123-POD\"
{APP name} = \"appName\""""@en .


###  http://sii.cecs.anu.edu.au/onto/SOLID-App#EncryptedFile
sa-onto:EncryptedFile rdf:type owl:Class ;
                      rdfs:subClassOf sa-onto:File ,
